h3 Archived Requests
    //small Some description about archived requests...

.container-fluid(ng-controller='ArchivedRequestsCtrl as ctrl')
    .panel.panel-default
        .panel-heading
            .panel-title Archived requests

        table.table.ng-table-rowselected(ng-table='ctrl.tableParams')
            tr(ng-if="!ctrl.loaded")
                td
                    .ball-pulse(style="margin: 0 auto; text-align: center;")
                        div
                        div
                        div
            tr(ng-repeat=" request in ctrl.archivedRequests")
                td(data-title="'Game1'") {{ request.requester_game.full_name }}
                td(data-title="'Game2'") {{ request.requested_game.full_name }}
                td(data-title="'Requester'") {{ request.requester.name }}
                td(data-title="'Requested'") {{ request.requested.name }}
                td(data-title="'Status'")
                    status-display(request="request")
                    md-tooltip(md-direction="top") {{ request.status_history_display }}
